{
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"layout": { 
			"contentSize": "680px", 
			"wideSize": "1120px" 
		},
		"color": {
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultGradients": false,
			"defaultPalette": false,
			"link": true,
			"palette": [
				{
					"slug": "foreground",
					"color": "#000000",
					"name": "Foreground"
				},
				{
					"slug": "background",
					"color": "#FFFFFF",
					"name": "Background"
				},
				{
					"slug": "primary",
					"color": "#F8CD5F",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#FFFCF5",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#F4F6FA",
					"name": "Tertiary"
				},
				{
					"slug": "transparent",
					"color": "transparent",
					"name": "Transparent"
				}
			],
			"gradients": [
				{
					"slug": "background-to-secondary",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--background), var(--wp--preset--color--secondary))",
					"name": "Background to Secondary"
				},
				{
					"slug": "secondary-to-background",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--secondary), var(--wp--preset--color--background))",
					"name": "Secondary to Quaternary"
				}
			],
			"duotone": [
				{
					"colors": [ "#000000", "#F8CD5F" ],
					"slug": "foreground-primary",
					"name": "Black and Yellow"
				}
			]
		},
		"spacing": {
			"blockGap": null,
			"margin": true,
			"padding": true,
			"units": [ "px", "em", "rem", "vh", "vw", "%" ]
		},
		"typography": {
			"dropCap": false,
			"customFontSize": true,
			"customLineHeight": true,
			"fontFamilies": [
				{
					"name": "System",
					"slug": "system",
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif"
				},
				{
					"name": "Serif",
					"slug": "serif",
					"fontFamily": "\"Times New Roman\",\"New York\",Times,\"Noto Serif\",serif"
				},
				{
					"name": "Monospace",
					"slug": "monospace",
					"fontFamily": "Consolas,Menlo,Monaco,\"SF Mono\",\"DejaVu Sans Mono\",\"Roboto Mono\",\"Courier New\",Courier,monospace"
				},
				{
					"name": "Montserrat",
					"slug": "montserrat",
					"fontFamily": "\"Montserrat\",sans-serif"
				}
			],
			"fontSizes":[
				{
					"name": "Extra Small",
					"size": "16px",
					"slug": "extra-small"
				},
				{
					"name": "Small",
					"size": "19px",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "22px",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "32px",
					"slug": "large"
				},
				{
					"name": "Huge",
					"size": "clamp(1.5rem, 2vw, 40px)",
					"slug": "huge"
				},
				{
					"name": "Gigantic",
					"size": "clamp(1.75rem, 3vw, 50px)",
					"slug": "gigantic"
				},
				{
					"name": "Colossal",
					"size": "clamp(2.5rem, 4vw, 72px)",
					"slug": "colossal"
				}
			],
			"lineHeight": true
		},
		"custom": {
			"spacing": {
				"small": "clamp(1rem, 40px, 2rem)",
				"medium": "clamp(2rem, 8vw, calc(4 * var(--wp--style--block-gap)))",
				"large": "clamp(4rem, 10vw, 8rem)",
				"outer": "clamp(2.5rem, 80px, 5vw)"
			},
			"line-height": {
				"tiny": 1.1,
				"small": 1.3,
				"medium": 1.6,
				"normal": 1.75
			},
			"fontWeight": {
				"regular": 400,
				"medium": 500,
				"semi-bold": 600
			}
		},
		"blocks": {}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--foreground)",
					"text": "var(--wp--preset--color--background)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "400"
				}
			},
			"core/column": {
				"spacing": {
					"margin": {
						"bottom": "40px"
					}
				}
			},
			"core/columns": {
				"spacing": {
					"margin": {
						"bottom": "0"
					}
				}
			},
			"core/cover": {
				"spacing": {
					"padding": {
						"top": "40px",
						"right": "40px",
						"bottom": "40px",
						"left": "40px"
					}
				}
			},
			"core/group": {
				"spacing": {
					"padding": "0px"
				}
			},
			"core/heading": {
				"spacing": {
					"margin": {
						"top": "10px",
						"bottom": "10px"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"fontWeight": "var(--wp--custom--font-weight--semi-bold)",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			},
			"core/image": {
				"spacing": {
					"margin": {
						"bottom": "20px"
					}
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/paragraph": {
				"spacing": {
					"margin": {
						"top": "0px",
						"bottom": "30px"
					}
				}
			},
			"core/post-comments": {
				"spacing": {
					"padding": {
						"top": "80px"
					}
				}
			},
			"core/post-featured-image": {
				"spacing": {
					"margin": {
						"bottom": "40px",
						"top": "0"
					}
				}
			},
			"core/post-title": {
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "40px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--colossal)",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			},
			"core/query-title": {
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "40px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--colossal)",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			},
			"core/separator": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"spacing": {
					"margin": {
						"top": "40px",
						"bottom": "40px"
					}
				}
			},
			"core/table": {
				"spacing": {
					"margin": {
						"bottom": "40px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/template-part": {
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "0"
					}
				}
			},
			"core/term-description": {
				"spacing": {
					"margin": {
						"top": "-20px",
						"bottom": "80px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				}
			},
			"h1": {
				"typography": {
					"fontWeight": "600",
					"fontSize": "var(--wp--preset--font-size--gigantic)"
				}
			},
			"h2": {
				"typography": {
					"fontWeight": "600",
					"fontSize": "var(--wp--preset--font-size--huge)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "500"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "500"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)",
					"fontWeight": "600",
					"textTransform": "uppercase"
				}
			}
		},
		"typography": {
			"fontSmoothing": true,
			"fontFamily": "var(--wp--preset--font-family--montserrat)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"fontWeight": "var(--wp--custom--font-weight--medium)",
			"lineHeight": "var(--wp--custom--line-height--normal)"
		}
	},
	"customTemplates": [
		{
			"name": "page-front",
			"title": "Front Page",
			"postTypes": [
				"page",
				"post"
			]
		},
		{
			"name": "blank",
			"title": "Blank template",
			"postTypes": [
				"page",
				"post"
			]
		},
		{
			"name": "no-title",
			"title": "No Title",
			"postTypes": [
				"page",
				"post"
			]
		}
	],
	"templateParts": [
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		},
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "header-transparent",
			"title": "Header Transparent",
			"area": "header"
		}
	]
}
