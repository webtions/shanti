{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"layout": {
			"contentSize": "680px",
			"wideSize": "1580px"
		},
		"color": {
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultGradients": false,
			"defaultPalette": false,
			"link": true,
			"palette": [
				{
					"slug": "foreground",
					"color": "#000000",
					"name": "Foreground"
				},
				{
					"slug": "background",
					"color": "#FFFFFF",
					"name": "Background"
				},
				{
					"slug": "primary",
					"color": "#F8CD5F",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#FFFCF5",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#F4F6FA",
					"name": "Tertiary"
				},
				{
					"slug": "contrast",
					"color": "#000000",
					"name": "Contrast"
				},
				{
					"slug": "contrast-medium",
					"color": "#4E4E60",
					"name": "Contrast Medium"
				},
				{
					"slug": "base-secondary",
					"color": "#F8F7FC",
					"name": "Base Secondary"
				},
				{
					"slug": "border-light",
					"color": "#E3E3F0",
					"name": "Border Light"
				},
				{
					"slug": "border-dark",
					"color": "#000000",
					"name": "Border Dark"
				},
				{
					"slug": "accent-light",
					"color": "#F9F3E3",
					"name": "Accent Light"
				},
				{
					"slug": "transparent",
					"color": "transparent",
					"name": "Transparent"
				}
			],
			"gradients": [
				{
					"slug": "background-to-secondary",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--background), var(--wp--preset--color--secondary))",
					"name": "Background to Secondary"
				},
				{
					"slug": "secondary-to-background",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--secondary), var(--wp--preset--color--background))",
					"name": "Secondary to Background"
				},
				{
					"slug": "primary-glow",
					"gradient": "radial-gradient(circle at center, color-mix(in srgb, var(--wp--preset--color--primary) 15%, transparent) 0%, transparent 70%)",
					"name": "Primary Glow"
				},
				{
					"slug": "warm-sunset",
					"gradient": "linear-gradient(135deg, #F8CD5F 0%, #FFA500 100%)",
					"name": "Warm Sunset"
				},
				{
					"slug": "soft-gradient",
					"gradient": "linear-gradient(to top, #accbee 0%, #e7f0fd 100%)",
					"name": "Soft Gradient"
				}
			],
			"duotone": [
				{
					"colors": ["#000000", "#F8CD5F"],
					"slug": "foreground-primary",
					"name": "Black and Yellow"
				},
				{
					"colors": ["#4E4E60", "#FFFFFF"],
					"slug": "dark-grayscale",
					"name": "Dark and Grayscale"
				}
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"customFontSize": true,
			"customLineHeight": true,
			"defaultFontSizes": false,
			"fontFamilies": [
				{
					"name": "Montserrat",
					"slug": "montserrat",
					"fontFamily": "\"Montserrat\", sans-serif",
					"fontFace": [
						{
							"src": ["file:./assets/fonts/Montserrat-VariableFont_wght.ttf"],
							"fontWeight": "100 900",
							"fontStyle": "normal",
							"fontFamily": "\"Montserrat\""
						},
						{
							"src": ["file:./assets/fonts/Montserrat-Italic-VariableFont_wght.ttf"],
							"fontWeight": "100 900",
							"fontStyle": "italic",
							"fontFamily": "\"Montserrat\""
						}
					]
				},
				{
					"name": "Playfair Display",
					"slug": "playfair-display",
					"fontFamily": "\"Playfair Display\", serif",
					"fontFace": [
						{
							"src": ["file:./assets/fonts/PlayfairDisplay-VariableFont_wght.ttf"],
							"fontWeight": "400 900",
							"fontStyle": "normal",
							"fontFamily": "\"Playfair Display\""
						},
						{
							"src": ["file:./assets/fonts/PlayfairDisplay-Italic-VariableFont_wght.ttf"],
							"fontWeight": "400 900",
							"fontStyle": "italic",
							"fontFamily": "\"Playfair Display\""
						}
					]
				},
				{
					"name": "Monospace",
					"slug": "monospace",
					"fontFamily": "Consolas,Menlo,Monaco,\"SF Mono\",\"DejaVu Sans Mono\",\"Roboto Mono\",\"Courier New\",Courier,monospace"
				}
			],
			"fontSizes": [
				{
					"name": "Extra Small",
					"size": "0.875rem",
					"slug": "x-small",
					"fluid": {
						"min": "0.875rem",
						"max": "1rem"
					}
				},
				{
					"name": "Small",
					"size": "1.125rem",
					"slug": "small",
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					}
				},
				{
					"name": "Medium",
					"size": "1.25rem",
					"slug": "medium",
					"fluid": {
						"min": "1.125rem",
						"max": "1.375rem"
					}
				},
				{
					"name": "Large",
					"size": "2rem",
					"slug": "large",
					"fluid": {
						"min": "1.75rem",
						"max": "2rem"
					}
				},
				{
					"name": "Extra Large",
					"size": "2.5rem",
					"slug": "x-large",
					"fluid": {
						"min": "2.25rem",
						"max": "3rem"
					}
				},
				{
					"name": "2X Large",
					"size": "3.5rem",
					"slug": "xx-large",
					"fluid": {
						"min": "3rem",
						"max": "4rem"
					}
				},
				{
					"name": "3X Large",
					"size": "4.5rem",
					"slug": "xxx-large",
					"fluid": {
						"min": "4rem",
						"max": "5rem"
					}
				}
			],
			"lineHeight": true
		},
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"units": ["px", "em", "rem", "vh", "vw", "%"],
			"spacingSizes": [
				{
					"name": "X-Small",
					"size": "clamp(0.5rem, 1.5vw, 1rem)",
					"slug": "x-small"
				},
				{
					"name": "Small",
					"size": "clamp(1rem, 2vw, 1.5rem)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(1.5rem, 3vw, 2rem)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(2rem, 4vw, 3rem)",
					"slug": "large"
				},
				{
					"name": "X-Large",
					"size": "clamp(2.5rem, 5vw, 4rem)",
					"slug": "x-large"
				},
				{
					"name": "XX-Large",
					"size": "clamp(3rem, 6vw, 5rem)",
					"slug": "xx-large"
				},
				{
					"name": "Outer",
					"size": "clamp(1.875rem, 5vw, 5rem)",
					"slug": "outer"
				}
			]
		},
		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"name": "Light",
					"slug": "light",
					"shadow": "0px 4px 8px 0px rgba(0, 0, 0, 0.05)"
				},
				{
					"name": "Medium",
					"slug": "medium",
					"shadow": "0px 8px 16px 0px rgba(0, 0, 0, 0.08)"
				},
				{
					"name": "Large",
					"slug": "large",
					"shadow": "0px 16px 32px 0px rgba(0, 0, 0, 0.12)"
				},
				{
					"name": "Natural",
					"slug": "natural",
					"shadow": "6px 6px 9px rgba(0, 0, 0, 0.2)"
				},
				{
					"name": "Hard Shadow",
					"slug": "hard",
					"shadow": "8px 8px 0px var(--wp--preset--color--primary)"
				}
			]
		},
		"custom": {
			"lineHeight": {
				"none": 1,
				"tight": 1.1,
				"snug": 1.2,
				"small": 1.1,
				"medium": 1.5,
				"body": 1.65,
				"normal": 1.75,
				"relaxed": 1.85,
				"loose": 2
			},
			"fontWeight": {
				"thin": 100,
				"light": 300,
				"regular": 400,
				"medium": 500,
				"semi-bold": 600,
				"bold": 700,
				"black": 900
			}
		},
		"blocks": {}
	},
	"styles": {
		"spacing": {
			"blockGap": "var:preset|spacing|medium",
			"padding": {
				"top": "0px",
				"bottom": "0px"
			}
		},
		"typography": {
			"fontSmoothing": true,
			"fontFamily": "var(--wp--preset--font-family--montserrat)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "var(--wp--custom--line-height--body)"
		},
		"blocks": {
			"core/button": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--foreground)",
					"text": "var(--wp--preset--color--background)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "var(--wp--custom--font-weight--medium)"
				},
				":focus": {
					"outline": {
						"color": "var(--wp--preset--color--primary)",
						"offset": "2px",
						"width": "2px",
						"style": "solid"
					}
				},
				"variations": {
					"outline": {
						"border": {
							"color": "var(--wp--preset--color--foreground)",
							"width": "1px",
							"style": "solid"
						},
						"color": {
							"background": "transparent",
							"text": "var(--wp--preset--color--foreground)"
						},
						"css": ":hover { background-color: var(--wp--preset--color--primary); color: var(--wp--preset--color--foreground); border-color: var(--wp--preset--color--primary); }"
					},
					"ghost": {
						"color": {
							"background": "transparent",
							"text": "var(--wp--preset--color--foreground)"
						},
						"spacing": {
							"padding": {
								"left": "0",
								"right": "0"
							}
						},
						"typography": {
							"fontWeight": "var(--wp--custom--font-weight--semi-bold)",
							"textDecoration": "underline"
						},
						"css": ":hover { text-decoration: none; } "
					}
				}
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "var:preset|spacing|small"
				},
				"css": "& .wp-block-button a:hover { background-color: var(--wp--preset--color--primary); color: var(--wp--preset--color--foreground); text-decoration: none; }"
			},
			"core/column": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--small)"
					}
				}
			},
			"core/columns": {
				"spacing": {
					"margin": {
						"bottom": "0"
					},
					"blockGap": "var(--wp--preset--spacing--small)"
				}
			},
			"core/cover": {
				"spacing": {
					"padding": {
						"top": "var:preset|spacing|outer",
						"right": "var:preset|spacing|outer",
						"bottom": "var:preset|spacing|outer",
						"left": "var:preset|spacing|outer"
					}
				},
				"elements": {
					"h1": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--xxx-large)"
						}
					}
				}
			},
			"core/group": {
				"spacing": {
					"padding": "0px"
				}
			},
			"core/heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--playfair-display)",
					"fontWeight": "var(--wp--custom--font-weight--medium)",
					"lineHeight": "var(--wp--custom--line-height--small)"
				},
				"css": "& { text-wrap: balance; }"
			},
			"core/image": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--x-small)"
					}
				}
			},
			"core/gallery": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--x-small)"
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "var(--wp--custom--font-weight--regular)"
				},
				"spacing": {
					"blockGap": "var:preset|spacing|small"
				},
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/paragraph": {
				"css": "&.has-background { padding: var(--wp--preset--spacing--medium); }"
			},
			"core/post-comments": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--large)"
					}
				}
			},
			"core/post-featured-image": {
				"border": {
					"radius": "0"
				},
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--small)",
						"top": "0"
					}
				}
			},
			"core/post-title": {
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "var(--wp--preset--spacing--small)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"lineHeight": "var(--wp--custom--line-height--small)",
					"fontWeight": "var(--wp--custom--font-weight--medium)"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/post-date": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--foreground)"
						},
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				},
				"css": "& { white-space: nowrap; flex-shrink: 0; }"
			},
			"core/post-terms": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				},
				"css": "&.has-hashtag-prefix a::before { content: \"#\"; }"
			},
			"core/post-time-to-read": {
				"color": {
					"text": "var(--wp--preset--color--contrast-medium)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-author-name": {
				"color": {
					"text": "var(--wp--preset--color--contrast-medium)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-author": {
				"color": {
					"text": "var(--wp--preset--color--contrast-medium)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-excerpt": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast-medium)"
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--x-small)",
						"bottom": "var(--wp--preset--spacing--x-small)"
					}
				}
			},
			"core/query-title": {
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "var(--wp--preset--spacing--small)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxx-large)",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			},
			"core/query-pagination": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "var(--wp--custom--font-weight--medium)"
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--small)"
					}
				}
			},
			"core/quote": {
				"border": {
					"width": "0 0 0 4px",
					"color": "var(--wp--preset--color--primary)"
				},
				"spacing": {
					"padding": {
						"left": "var(--wp--preset--spacing--small)",
						"top": "var(--wp--preset--spacing--x-small)",
						"bottom": "var(--wp--preset--spacing--x-small)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontStyle": "italic",
					"fontWeight": "var(--wp--custom--font-weight--medium)"
				}
			},
			"core/pullquote": {
				"border": {
					"width": "0",
					"style": "none"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"lineHeight": "var(--wp--custom--line-height--tight)",
					"fontWeight": "var(--wp--custom--font-weight--bold)"
				}
			},
			"core/code": {
				"border": {
					"radius": "4px"
				},
				"color": {
					"background": "var(--wp--preset--color--base-secondary)",
					"text": "var(--wp--preset--color--foreground)"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--x-small)",
						"right": "var(--wp--preset--spacing--x-small)",
						"bottom": "var(--wp--preset--spacing--x-small)",
						"left": "var(--wp--preset--spacing--x-small)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/preformatted": {
				"border": {
					"radius": "4px",
					"color": "var(--wp--preset--color--border-light)",
					"width": "1px",
					"style": "solid"
				},
				"color": {
					"background": "var(--wp--preset--color--base-secondary)",
					"text": "var(--wp--preset--color--foreground)"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--small)",
						"right": "var(--wp--preset--spacing--small)",
						"bottom": "var(--wp--preset--spacing--small)",
						"left": "var(--wp--preset--spacing--small)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/search": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--x-small)"
					}
				},
				"border": {
					"radius": "0"
				}
			},
			"core/separator": {
				"border": {
					"color": "currentColor",
					"style": "solid",
					"width": "0 0 2px 0"
				},
				"color": {
					"text": "var:preset|color|primary"
				},
				"spacing": {
					"margin": {
						"top": "var:preset|spacing|x-large",
						"bottom": "var:preset|spacing|x-large"
					}
				},
				"css": ":not(.is-style-wide):not(.is-style-dots) {width: 100px;}",
				"variations": {
					"wide": {
						"css": " &:not(.alignfull){max-width: var(--wp--style--global--wide-size) !important;}"
					}
				}
			},
			"core/table": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--small)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"border": {
					"color": "var(--wp--preset--color--border-light)"
				}
			},
			"core/template-part": {
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "0"
					}
				}
			},
			"core/term-description": {
