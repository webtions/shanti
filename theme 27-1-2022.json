{
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"layout": { 
			"contentSize": "680px", 
			"wideSize": "1120px" 
		},
		"color": {
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultGradients": false,
			"defaultPalette": false,
			"link": true,
			"palette": [
				{
					"slug": "foreground",
					"color": "#000000",
					"name": "Foreground"
				},
				{
					"slug": "background",
					"color": "#FFFFFF",
					"name": "Background"
				},
				{
					"slug": "primary",
					"color": "#F8CD5F",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#FFFCF5",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#F4F6FA",
					"name": "Tertiary"
				},
				{
					"slug": "transparent",
					"color": "transparent",
					"name": "Transparent"
				}
			],
			"gradients": [
				{
					"slug": "background-to-secondary",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--background), var(--wp--preset--color--secondary))",
					"name": "Background to Secondary"
				},
				{
					"slug": "secondary-to-background",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--secondary), var(--wp--preset--color--background))",
					"name": "Secondary to Quaternary"
				}
			],
			"duotone": [
				{
					"colors": [ "#000000", "#F8CD5F" ],
					"slug": "foreground-primary",
					"name": "Black and Yellow"
				}
			]
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"customFontSize": true,
			"customLineHeight": true,
			"fontFamilies": [
				{
					"name": "System",
					"slug": "system",
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif"
				},
				{
					"name": "Serif",
					"slug": "serif",
					"fontFamily": "\"Times New Roman\",\"New York\",Times,\"Noto Serif\",serif"
				},
				{
					"name": "Monospace",
					"slug": "monospace",
					"fontFamily": "Consolas,Menlo,Monaco,\"SF Mono\",\"DejaVu Sans Mono\",\"Roboto Mono\",\"Courier New\",Courier,monospace"
				},
				{
					"name": "Montserrat",
					"slug": "montserrat",
					"fontFamily": "\"Montserrat\",sans-serif"
				}
			],
			"fontSizes":[
				{
					"slug": "extra-small",
					"size": "1rem",
					"name": "Extra small"
				},
				{
					"slug": "small",
					"size": "1.125rem",
					"name": "Small"
				},
				{
					"slug": "medium",
					"size": "1.25rem",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "2rem",
					"name": "Large"
				},
				{
					"slug": "x-large",
					"size": "clamp(1.75rem, 3vw, 3.25rem)",
					"name": "XL"
				},
				{
					"slug": "xx-large",
					"size": "clamp(1.75rem, 3.75rem, 3.75rem)",
					"name": "XXL"
				}
			]
		},
		"custom": {
			"spacing": {
				"small": "clamp(1rem, 40px, 2rem)",
				"medium": "clamp(2rem, 8vw, calc(4 * var(--wp--style--block-gap)))",
				"large": "clamp(4rem, 10vw, 8rem)",
				"outer": "clamp(2.5rem, 80px, 5vw)"
			},
			"typography": {
				"line-height": {
					"tiny": 1.15,
					"small": 1.2,
					"medium": 1.4,
					"normal": 1.8
				}
			}
		},
		"blocks": {}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"spacing": {
			"blockGap": "1.125rem"
		},
		"typography": {
			"fontSmoothing": true,
			"fontFamily": "var(--wp--preset--font-family--montserrat)",
			"lineHeight": "var(--wp--custom--typography--line-height--normal)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"fontWeight": "500"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"fontWeight": "600",
					"lineHeight": "80px",
					"fontSize": "4rem"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"fontWeight": "600",
					"lineHeight": "60px",
					"fontSize": "3rem"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"fontWeight": "500",
					"lineHeight": "50px",
					"fontSize": "2.5rem"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"fontWeight": "500",
					"lineHeight": "40px",
					"fontSize": "2rem"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"fontWeight": "500",
					"lineHeight": "30px",
					"fontSize": "1.25rem"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"fontWeight": "600",
					"textTransform": "uppercase",
					"lineHeight": "20px",
					"fontSize": "1rem"
				}
			}
		},
		"custom": {},
		"blocks": {
			"core/button": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--foreground)",
					"text": "var(--wp--preset--color--background)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "400"
				}
			},
			"core/post-comments": {
				"spacing": {
					"padding": {
						"top": "80px"
					}
				}
			},
			"core/template-part": {
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "0"
					}
				}
			}
		}
	},
	"customTemplates": [
		{
			"name": "page-front",
			"title": "Front Page",
			"postTypes": [
				"page",
				"post"
			]
		},
		{
			"name": "blank",
			"title": "Blank template",
			"postTypes": [
				"page",
				"post"
			]
		},
		{
			"name": "no-title",
			"title": "No Title",
			"postTypes": [
				"page",
				"post"
			]
		}
	],
	"templateParts": [
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		},
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "header-cover",
			"title": "Header with large cover",
			"area": "header"
		}
	]
}
